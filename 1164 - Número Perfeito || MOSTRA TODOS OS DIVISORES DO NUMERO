#include <stdio.h>

int main(){
	int n,x,divisores,i,aux;
	
	scanf("%d", &n);
	while(n != 0){
		scanf("%d", &x);
		divisores = 0;
		for(i=1;i != x;i++){
			aux = x;
			if(aux % i == 0){
				divisores += i;
				aux /= i;
			}	
		}
		if(divisores == x)
			printf("%d eh perfeito\n", x);
		else
			printf("%d nao eh perfeito\n", x);
		n--;
	}
	
	return 0;
}
