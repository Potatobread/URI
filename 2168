#include <stdio.h>

int main(){
	int n,aux,i,j,control=1,camera=0;
	
	scanf("%d", &n);
	int v[n][n];
	for(i=0;i <= n;i++)
		for(j=0;i <= n;j++)
			scanf("%d", &v[i][j]);
	
	aux = n*n;	
	while(aux != 0){
		for(i=0;i <= control;i++)
			for(j=0;j <= control;j++)
				if(v[i][j] == 1)
					camera++;
		if(camera >= 2)
			printf("S");
		else
			printf("U");	
		aux--;
	}
			
	
	return 0;
}


//0 0 / 0 1
//1 0 / 1 1
