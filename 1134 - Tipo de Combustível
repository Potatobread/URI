#include <stdio.h>

int main(){
	int x,c1=0,c2=0,c3=0;
	
	while(x != 4){
		scanf("%d", &x);
		if(x == 1)
			c1++;
		else if(x == 2)
		 	c2++;
		else if(x == 3)
			c3++;
	}
	printf("MUITO OBRIGADO\n");
	printf("Alcool: %d\n", c1);
	printf("Gasolina: %d\n", c2);
	printf("Diesel: %d\n", c3);
	
	return 0;
}
