#include <stdio.h>

int main(){
	double m[12][12],a=0;
	int l,i,j;
	char t;
	
	scanf("%d", &l);
	scanf(" %c", &t);
	
	for(i=0;i<12;i++){
		for(j=0;j<12;j++){
			scanf(" %lf", &m[i][j]);
			if(i == l)
				a += m[i][j];
		}	
	}
	
	if(t == 'S')
		printf("%.1lf\n", a);
	else if(t == 'M'){
		a /= 12;
		printf("%.1lf\n", a);
	}
	
	return 0;
}
