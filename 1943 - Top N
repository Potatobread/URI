#include <stdio.h>

int main(){
	int K,N;
	
	scanf("%d", &K);
	if(K == 1)
		N = K;
	else if(K == 2 || K == 3)
		N = 3;
	else if(K == 4 || K == 5)
		N = 5;
	else if(K > 5 && K <= 10)
		N = 10;
	else if(K > 10 && K <= 25)
		N = 25;
	else if(K > 25 && K <= 50)
		N = 50;
	else if(K > 50 && K <= 100)
		N = 100;
	
	printf("Top %d\n", N);
	
	return 0;
}
